   <script>
        ; (function () {
            function getLanguage() {
                var language = 'en';
                var cultureReplaceRegex = new RegExp("/[a-z]{2}-[a-z]{2}(/|$)|/en(/|$)");
                var path = window.location.pathname.toLowerCase();

                if (path.match(cultureReplaceRegex)) {
                    language = path.slice(1,6);
                }
                else if (navigator.language) {
                    language = navigator.language;
                }
                else if (navigator.userLanguage) {
                    language = navigator.userLanguage;
                }

                return language;
            }
            var host = 'https://lego.com/';
            if(location.host.indexOf('dev.corp.lego.com')>-1){
                host = 'https://dev.corp.lego.com/';
            }
            if(location.host.indexOf('webqa.lego.com')>-1){
                host = 'https://webqa.lego.com/';
            }


            var language = getLanguage();           

            // get the gns with the proper language
            // check if there is a valid culture of five chars
            if (language.length !== 5)
            {
                language = 'en-us';
            }
            var src = host + language + '/go/190/error';
            var script = document.createElement('script');
            script.src = src;
            document.head.appendChild(script);
        })();
    </script>

            
